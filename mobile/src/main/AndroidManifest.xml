<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.colescoreboard.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RefMainGameScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SportSelect" />
        <activity
            android:name=".BaseballSBintialinputs"
            android:label="@string/title_activity_baseball_sbintialinputs"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Intialize_connection" />
        <activity android:name=".baseball_starter" />
        <activity android:name=".baseball_game_manager" />
        <activity android:name=".soccer_game_manager"></activity>
    </application>

</manifest>